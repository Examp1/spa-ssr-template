{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0",
        "aranyasen/laravel-adminer": "^2.0",
        "astrotomic/laravel-translatable": "^11.9",
        "barryvdh/laravel-dompdf": "^2.0",
        "butschster/meta-tags": "^2.2",
        "cviebrock/eloquent-sluggable": "^8.0",
        "daaner/turbosms": "^1.11",
        "doctrine/dbal": "^3.2",
        "fideloper/proxy": "^4.4",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "intervention/image": "^2.5",
        "intervention/imagecache": "^2.5",
        "kalnoy/nestedset": "^5.0",
        "laravel/framework": "^8.75",
        "laravel/helpers": "^1.4",
        "laravel/socialite": "^5.6",
        "laravel/tinker": "^2.5",
        "laravel/ui": "^3.2",
        "maatwebsite/excel": "^3.1",
        "mailgun/mailgun-php": "^3.5",
        "maksa988/laravel-esputnik": "^1.0",
        "nyholm/psr7": "^1.5",
        "owlwebdev/ecom": "dev-master",
        "spatie/laravel-permission": "^5.5",
        "tymon/jwt-auth": "^1.0",
        "unisharp/laravel-filemanager": "^2.2"
    },
    "repositories": [
        {
            "type": "path",
            "url": "packages/owlwebdev/ecom",
            "options": {
                "symlink": true
            }
        }
    ],
    "require-dev": {
        "facade/ignition": "^2.5",
        "fakerphp/faker": "^1.9.1",
        "laravel/pint": "^1.5",
        "laravel/sail": "^1.16",
        "laravel/telescope": "^4.14",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.3.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "exclude-from-classmap": [
            "vendor/unisharp/laravel-filemanager/src/LfmPath.php"
        ],
        "files": [
            "app/Overrides/UniSharp/LaravelFilemanager/src/LfmPath.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "platform-check": false,
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
